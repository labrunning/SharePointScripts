<View Name="By Academic Year and Committee Date" DefaultView="TRUE">
	<ViewFields>
		<FieldRef Name="DocIcon"/>
		<FieldRef Name="LinkFilename"/>
		<FieldRef Name="Committee_x0020_Document_x0020_Type1"/>
		<FieldRef Name="Committee_x0020_Date1"/>
		<FieldRef Name="Version"/>
		<FieldRef Name="Modified"/>
		<FieldRef Name="Modified By"/>
		<FieldRef Name="_dlc_DocId"/>
	</ViewFields>
	<Query>
		<!-- **YOU CAN ONLY GROUP BY TWO LEVELS** -->
		<GroupBy Collapse='TRUE' GroupLimit='30'>
			<FieldRef Name='Committee_x0020_Academic_x0020_Year1' Ascending='FALSE'/>
			<FieldRef Name='Committee_x0020_Date1' Ascending='FALSE'/>
		</GroupBy>
		<OrderBy>
			<FieldRef Name='Committee_x0020_Date1' Ascending='FALSE'/>
			<FieldRef Name='Committee_x0020_Document_x0020_Type1'/>
		</OrderBy>
	</Query>
	<RowLimit Paged="TRUE">50</RowLimit>
</View>