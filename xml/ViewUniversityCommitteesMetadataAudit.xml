<View Name='Metadata Audit' DefaultView='FALSE'>
	<Query>
		<OrderBy>
			<FieldRef Name='ID' Ascending='TRUE'/>
		</OrderBy>
	</Query>
	<ViewFields>
		<FieldRef Name='DocIcon'/>
		<FieldRef Name='LinkFilename'/>
		<FieldRef Name='Title'/>
		<FieldRef Name='University Committee Name'/>
		<FieldRef Name='Committee Academic Year'/>
		<FieldRef Name='Committee Date'/>
		<FieldRef Name='Committee Document Type'/>
		<FieldRef Name="Version"/>
		<FieldRef Name="Document ID"/>		
		<FieldRef Name='ID'/>
	</ViewFields>
	<RowLimit Paged='TRUE'>100</RowLimit>
	<Aggregations>
		<FieldRef Name='LinkFilename' Type='COUNT'/>
	</Aggregations>
</View>